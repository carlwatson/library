/**
 * Implements hook_block_info().
 *
 * Create all the blocks required.
 */
function sybase2_extras_form_alter(&$form,&$form_state,$form_id){
  switch($form_id){
      case 'views_exposed_form':
          switch($form['#id']){
                case 'views-exposed-form-calendar-month':
                case 'views-exposed-form-calendar-year':
                case 'views-exposed-form-calendar-day':
                case 'views-exposed-form-calendar-week':
                  $empty_team_filter = false;
                  $empty_event_type = false;
                  $empty_group_filter = false;
                  if(sizeof($form['team_filter']['#options']) == 0){
                    hide($form['team_filter']);
                    $empty_team_filter = true;
                  }
                  if(sizeof($form['event_type']['#options']) == 0){
                    hide($form['event_type']);
                    $empty_event_type = true;
                  }
                  if(sizeof($form['group_filter']['#options']) == 0){
                    hide($form['group_filter']);
                    $empty_group_filter = true;
                  }
                  if($empty_team_filter == true && $empty_event_type == true && $empty_group_filter == true){
                      hide($form['submit']);
                      hide($form['reset']);
                  }
                  break;
          }
          /* echo '<pre>';
          print_r($form);
          echo '</pre>'; */
          break;
      }
}
