        $content .= "<i class='fa fa-" . $social_menu['social_menu_4_icon'] . "'></i>";
        $content .= "</a></div>";
      }

      if ($social_menu['social_menu_5_link']) {
        $content .= "<div class='social-icon-item social-icon-item-5'><a href='" . $social_menu['social_menu_5_link'] . "' target='_blank'>";
        $content .= "<i class='fa fa-" . $social_menu['social_menu_5_icon'] . "'></i>";
        $content .= "</a></div>";
      }

      if ($social_menu['social_menu_6_link']) {
        $content .= "<div class='social-icon-item social-icon-item-6'><a href='" . $social_menu['social_menu_6_link'] . "' target='_blank'>";
        $content .= "<i class='fa fa-" . $social_menu['social_menu_6_icon'] . "'></i>";
        $content .= "</a></div>";
      }

      $content .= "</div>";


      $block['content'] = $content;
      break;

    case 'sy_floating_block':
      $floating_block = variable_get('floating_block_options', array(
        'floating_block_font' => '',
        'floating_block_1' => '',
        'floating_block_1_link' => '',
        'floating_block_1_icon' => '',
        'floating_block_2' => '',
        'floating_block_2_link' => '',
        'floating_block_2_icon' => '',
        'floating_block_3_on' => '',
        'floating_block_3' => '',
        'floating_block_3_link' => '',
        'floating_block_3_icon' => '',
        'floating_block_4_on' => '',
        'floating_block_4' => '',
        'floating_block_4_link' => '',
        'floating_block_4_icon' => '',
        'floating_block_5_on' => '',
        'floating_block_5' => '',
        'floating_block_5_link' => '',
        'floating_block_5_icon' => '',
        'floating_block_6_on' => '',
        'floating_block_6' => '',
        'floating_block_6_link' => '',
        'floating_block_6_icon' => '',
        'floating_block_login' => '',
        'floating_block_logout' => '',
        'floating_block_logout_icon' => '',
      ));

      if ($floating_block['floating_block_font'] == 1) {
        $fa = 'fa fa-';
      } else {
        $fa = 'icon-';
       }
      $content = "<div id='block-sybase2-sy-floating-block' class='active'>";

      $content.="<div class='float-close'><i class='fa fa-close'></i></div>";

      $content .='<ul class="floating-block-wrapper">';

      $content .='<li class="btn-1 btn">';
      $content .='<a href="' . $floating_block['floating_block_1_link'] . '">';
      $content .= '<i class="'. $fa . $floating_block['floating_block_1_icon'].'"></i>' . $floating_block['floating_block_1'];
      $content .='</a>';
      $content .='</li>';

      $content .='<li class="btn-2 btn">';
      $content .='<a href="' . $floating_block['floating_block_2_link'] . '">';
      $content .= '<i class="'. $fa .$floating_block['floating_block_2_icon'].'"></i>' . $floating_block['floating_block_2'];
      $content .='</a>';
      $content .='</li>';

      if ($floating_block['floating_block_3_on'] == 1) {
        $content .='<li class="btn-3 btn">';
        $content .='<a href="' . $floating_block['floating_block_3_link'] . '">';
        $content .= '<i class="'. $fa .$floating_block['floating_block_3_icon'].'"></i>' . $floating_block['floating_block_3'];
        $content .='</a>';
        $content .='</li>';
      }

      if ($floating_block['floating_block_4_on'] == 1) {
        $content .='<li class="btn-4 btn">';
        $content .='<a href="' . $floating_block['floating_block_4_link'] . '">';
        $content .= '<i class="'. $fa .$floating_block['floating_block_4_icon'].'"></i>' . $floating_block['floating_block_4'];
        $content .='</a>';
        $content .='</li>';
      }

      if ($floating_block['floating_block_5_on'] == 1) {
        $content .='<li class="btn-5 btn">';
        $content .='<a href="' . $floating_block['floating_block_5_link'] . '">';
        $content .= '<i class="'. $fa .$floating_block['floating_block_5_icon'].'"></i>' . $floating_block['floating_block_5'];
        $content .='</a>';
        $content .='</li>';
      }

      if ($floating_block['floating_block_6_on'] == 1) {
        $content .='<li class="btn-6 btn">';
        $content .='<a href="' . $floating_block['floating_block_6_link'] . '">';
        $content .= '<i class="'. $fa .$floating_block['floating_block_6_icon'].'"></i>' . $floating_block['floating_block_6'];
        $content .='</a>';
        $content .='</li>';
      }

      $content .='<li class="login_logout_btn btn">';
      $link = user_is_logged_in() ? '/user/logout' : '/user';
      $text = user_is_logged_in() ? $floating_block['floating_block_logout'] : $floating_block['floating_block_login'];
      $content .='<a href="' . $link . '">';
      $content .= '<i class="'.$fa .$floating_block['floating_block_logout_icon'].'"></i>' . $text;
      $content .='</a>';
      $content .='</li>';
      $content .= "</ul></div>";
      $block['content'] = $content;
      break;

    case 'hos_popup1':
      $text = sy_variable_get('hos_popup1');

      $block['content'] = '<div class="builder-pop">';
      $block['content'] .= '<div class="builder-overlay"></div><div class="builder-pop-internal"><a class="pop-close" href="/"><i class="fa fa-close"></i></a>';
      if (!$text) {
        $text = '<strong>Northampton Academy</strong><br />1200 North Drive<br />Northampton, MA 01060<br />413-555-1212';
      }
      $block['content'] .= '<p>' . check_markup($text,'full_html') . '</p>';
      $block['content'] .= '</div></div>';
      break;

  }

  return $block;
}